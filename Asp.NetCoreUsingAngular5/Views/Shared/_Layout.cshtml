    <script src="~/dist/admin/dist/js/sidebarmenu.js" asp-append-version="true"></script>
    <!--Custom JavaScript -->
    <script src="~/dist/admin/dist/js/custom.min.js" asp-append-version="true"></script>
    @RenderSection("scripts", required: false)
